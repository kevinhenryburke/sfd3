<design:component >
    <design:attribute name="Title" default="1" label="Title Text" description=""/>        
    <design:attribute name="UserComponentId" label="Component Id" default="" description="Unique Id for this component" />	
    <design:attribute name="dataSourceMethod" label="Data Source Method" default="c.returnDataNodesOnly" description="Apex Method returning base data" />      
    <design:attribute name="dataUpdateMethod" label="Data Update Method" default="c.returnDataUpdate" description="Apex Method returning updated data" />      
    <design:attribute name="primaryNodeInitialization" label="Primary Node to highlight" default="FirstInData" description="FirstInData (which is Default), RecordId, None" />          
    <design:attribute name="configjsonString" label="Config JSON" description="configuration JSON, do not change this unless you really understand what it does!"/>        
